  });
</script>

<script>
// Hide errors
$('[wr-type="error"]').hide();

$('.error').removeClass('error'); // Remove error state from fields

var formErrors = false;

const fieldError = function(field) {
  field.parent().find('[wr-type="error"]').show(); // Show error message
  field.addClass('error'); // Add error state to this field
  formErrors = true;
};

// Click on the Submit button
$('[wr-type="submit"]').click(function() {
  // Check each required field
  $('[wr-type="required-field"]').each(function() {
    if ($(this).attr('type') === 'checkbox' && !$(this).is(':checked')) { // Checkbox
      fieldError($(this).siblings('.w-checkbox-input'));
      return;
    }
    if ($(this).val().length === 0) { // If this field is empty
      fieldError($(this));
    } else if ($(this).attr('type') === 'email' // Validation for email fields
      && ($(this).val().indexOf('@') === -1 || $(this).val().indexOf('.') === -1)) {
      fieldError($(this));
    }
  });
  // If there are form errors, show the error message
  if (formErrors) {
    $('.error-message').show(); // Show the error message element
  } else {
    $('.error-message').hide(); // Hide the error message element
    $(this).parents('form').submit(); // Submit the form if there are no errors
  }
});

// Remove errors from field
$('[wr-type="required-field"], [type="checkbox"]').on('keypress blur', function() {
  $(this).parent().find('.error').removeClass('error');
  $(this).parent().find('[wr-type="error"]').hide();
  formErrors = false;
});

// Press Enter
$('input, textarea').keypress(function(e) {
  if (e.keyCode == 13) {
    e.preventDefault();
    $(this).trigger("change");
    $('[wr-type="submit"]').click();
  }
});
</script>

<script> 
//form code 
document.addEventListener("DOMContentLoaded", function() {
  const formFields = document.querySelectorAll('.form_field');
  const submitButton = document.querySelector('.button.is-form-submit');

  function updateLabelOpacity(field, state) {
    const label = field.previousElementSibling;
    if (label && label.classList.contains('field_label')) {
      label.style.opacity = state ? '1' : '0';
    }
  }

  function checkFormCompletion() {
    let allFilled = true;
    formFields.forEach(field => {
      if (field.value === '') {
        allFilled = false;
      }
    });
    submitButton.style.opacity = allFilled ? '1' : '0';
  }

  formFields.forEach(field => {
    // Update label opacity on focus
    field.addEventListener('focus', () => {
      updateLabelOpacity(field, true);
    });

    // Update label opacity when input has text
    field.addEventListener('input', () => {
      checkFormCompletion();
      if (field.value !== '') {
        updateLabelOpacity(field, true);
      } else {
        updateLabelOpacity(field, false);
      }
    });

    // Optional: revert opacity if field loses focus and is empty
    field.addEventListener('blur', () => {
      if (field.value === '') {
        updateLabelOpacity(field, false);
      }
    });
  });

  // Check form completion on page load
  checkFormCompletion();
});
</script>


